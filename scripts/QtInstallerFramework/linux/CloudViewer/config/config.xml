<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>CloudViewer</Name>
    <Version>3.7.1</Version>
    <Title>CloudViewer-3.7.1 Setup</Title>
    <Publisher>Asher</Publisher>
    <WizardStyle>Modern</WizardStyle>
    <StyleSheet>style.qss</StyleSheet>
    <TitleColor>#FFFFFF</TitleColor>
    <ProductUrl>https://github.com/Asher-1/CloudViewerUpdate</ProductUrl>
    <InstallerWindowIcon>erow_blue_1024.png</InstallerWindowIcon>
    <InstallerApplicationIcon>erow-blue.png</InstallerApplicationIcon>
    <RunProgram>@TargetDir@/CloudViewer-x86_64.AppImage</RunProgram>
    <RunProgramArguments></RunProgramArguments>
    <RunProgramDescription>Start CloudViewer</RunProgramDescription>
    <StartMenuDir>Graphics</StartMenuDir>
    <AllowNonAsciiCharacters>true</AllowNonAsciiCharacters>
    <TargetDir>@HomeDir@/CloudViewer</TargetDir>
    <AdminTargetDir>@rootDir@/opt/CloudViewer</AdminTargetDir>
</Installer>
