{
  "name": "cloudViewer",
  "version": "@PROJECT_VERSION_THREE_NUMBER@",
  "description": "CloudViewer: A Modern Library for 3D Data Processing",
  "author": "CloudViewer.org",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Asher-1/ACloudViewer.git"
  },
  "license": "MIT",
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../cloudViewer/labextension/ && rimraf ../cloudViewer/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "NODE_ENV=production webpack --mode=production --optimization-minimize && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze": "webpack-bundle-analyzer dist/index.js"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "webpack": "^5",
    "webpack-bundle-analyzer": "^4.5.0",
    "terser-webpack-plugin": "^5.0.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2 || ^3 || ^4 || ^5 || ^6",
    "lodash": "^4.17.4",
    "webrtc-adapter": "^4.2.2"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../cloudViewer/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
